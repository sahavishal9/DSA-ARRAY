def maxOccuringOneSubarray(arr):
    n=len(arr)
    ans=0
    cnt=0
    if arr[0]==1:
        cnt=1 
    for i in range(1,n):
        if  arr[i]==1:
            cnt+=1 
        else:
            if cnt>ans:
                ans=cnt
            cnt=0
    if cnt>ans:
        ans=cnt
    return ans

arr=[1,0,1,1,1,1]
print(maxOccuringOneSubarray(arr))

arr=[1,0,1,1,0,1]
print(maxOccuringOneSubarray(arr))
